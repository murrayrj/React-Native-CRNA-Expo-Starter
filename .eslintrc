{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
      "__DEV__": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        quotes: ["error", "single"],
        object-curly-spacing: ["error", "never"],
        "react/prop-types": "off"
    }
}
